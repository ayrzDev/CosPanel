{
  "name": "umixpanel",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "dev:docker": "docker compose up -d postgres redis",
    "dev:all": "pnpm run dev:docker && pnpm -r --parallel dev",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "e2e": "pnpm -r e2e",
    "typecheck": "pnpm -r typecheck",
    "prisma:migrate": "pnpm --filter @umixpanel/api prisma:migrate",
    "prisma:seed": "pnpm --filter @umixpanel/api prisma:seed"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "typescript": "^5.4.5",
    "rimraf": "^5.0.5",
    "concurrently": "^8.2.2"
  }
}
